<?xml version="1.0"?>
<robot name="biped-one">
	<link name="torso-base">
		<visual>
			<geometry>
                <box size="0.12 0.25 0.4"/>
			</geometry>
            <material name="blue">
                <color rgba="0 0 .5 1"/>
            </material>
		</visual>
        <collision>
            <geometry>
                <box size="0.12 0.25 0.4"/>
            </geometry>
        </collision>
        <inertial>
            <!--mass value="20"/-->
            <mass value="0"/>
            <!--inertia ixx="0.37" ixy="0.0" ixz="0.0" iyy="0.29" iyz="0.0" izz="0.128"/-->
